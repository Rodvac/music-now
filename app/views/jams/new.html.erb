  <%= simple_form_for [ @jam ] do |f| %>
  <%= f.input :name, as: :string %>
  <%= f.input :description, as: :string %>
  <%= simple_fields_for :other do |o| %>
    <%= o.input :instrument, as: :string, label: 'What instrument will u be playing?' %>
  <% end %>
  <%= f.submit "Creer session", class: 'btn btn-primary book-btn' %>
  <% end %>